.container {
  margin: 0;
  padding: 0;
}

.container .header {
  background-color: #2a5caa;
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  color: #fff;
  align-items: center;
}

.container .header .avatar {
  margin-top: 30rpx;
  width: 200rpx;
  height: 200rpx;
  background-color: #fff;
  border-radius: 50%;
}
.center .transparent-btn {
  width: 50rpx;
  height: 50rpx;
  /* background-color: pink; */
}
.center {
  font-size: 12px;
}

.center .icon {
  width: 80rpx;
  height: 80rpx;
}